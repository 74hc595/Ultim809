                            297 ; timer response routines
                            298 ;------------------------------------------------------------------------------
                            299 ; new level timer
   022F 0D 8A         [ 6]  300 CHECK_TIMERS:	tst	*NEW_LEVEL_TIMER ;new level?
   0231 27 04         [ 3]  301 		beq	chkrespawn	;timer is 0, so no
   0233 0A 8A         [ 6]  302 		dec	*NEW_LEVEL_TIMER ;timer is 1? yes
   0235 27 0C         [ 3]  303 		beq	startnewlevel
                            304 
                            305 ; respawn player timer
   0237 96 89         [ 4]  306 chkrespawn:	lda	*RESPAWN_TIMER	;need to respawn the player?
   0239 27 3E         [ 3]  307 		beq	chksaucer	;timer is 0, so no
   023B 81 01         [ 2]  308 		cmpa	#1
   023D 27 23         [ 3]  309 		beq	respawn		;timer is 1, try to respawn
   023F 0A 89         [ 6]  310 		dec	*RESPAWN_TIMER	;timer is > 1, decrement timer
   0241 20 36         [ 3]  311 		bra	chksaucer
                            312 
   0243 D6 8E         [ 4]  313 startnewlevel:	ldb	*START_ASTEROIDS	;next level has 1 more asteroid
   0245 5C            [ 2]  314 		incb
   0246 C1 06         [ 2]  315 		cmpb	#MAX_LG_ASTS
   0248 25 02         [ 3]  316 		blo	newastset
   024A C6 06         [ 2]  317 		ldb	#MAX_LG_ASTS
   024C D7 8E         [ 4]  318 newastset:	stb	*START_ASTEROIDS
   024E BD 05 2E      [ 8]  319 		jsr	AST_NEW_SET
   0251 86 F0         [ 2]  320 		lda	#240		;first saucer after 8 seconds
   0253 97 9A         [ 4]  321 		sta	*NEXT_SAUCER_INT
   0255 97 99         [ 4]  322 		sta	*SAUCER_TIMER
   0257 96 9C         [ 4]  323 		lda	*NEXT_LG_SAUCERS ;set number of large saucers
   0259 97 9B         [ 4]  324 		sta	*LARGE_SAUCERS
   025B 0F 8A         [ 6]  325 		clr	*NEW_LEVEL_TIMER
   025D BD 09 46      [ 8]  326 		jsr	THUMPSND_ON	;play thump sound
   0260 20 D5         [ 3]  327 		bra	chkrespawn
                            328 
   0262 0D 87         [ 6]  329 respawn:	tst	*LIVES		;can't respawn with zero lives
   0264 27 13         [ 3]  330 		beq	chksaucer
   0266 BD 06 AF      [ 8]  331 		jsr	SHIP_CAN_SPAWN	;is it safe to respawn?
   0269 26 0E         [ 3]  332 		bne	chksaucer	;if not, try again next frame
   026B BD 09 46      [ 8]  333 		jsr	THUMPSND_ON	;play thump sound
   026E 0F 89         [ 6]  334 		clr	*RESPAWN_TIMER
   0270 96 9A         [ 4]  335 		lda	*NEXT_SAUCER_INT ;reset saucer timer
   0272 97 99         [ 4]  336 		sta	*SAUCER_TIMER
   0274 BD 05 D1      [ 8]  337 		jsr	SHIP_INIT	;the ship might die here if in hypersp.
   0277 0F 98         [ 6]  338 		clr	*HYPERSPACE
                            339 
   0279 D6 90         [ 4]  340 chksaucer:	ldb	*FRAMECOUNTER	;update countdown every other frame
   027B C5 01         [ 2]  341 		bitb	#0b00000001
   027D 26 0B         [ 3]  342 		bne	timersdone
   027F 0D 99         [ 6]  343 		tst	*SAUCER_TIMER	;is timer zero?
   0281 27 07         [ 3]  344 		beq	timersdone	;if so, don't update
   0283 0A 99         [ 6]  345 		dec	*SAUCER_TIMER	;decrement saucer counter
   0285 26 03         [ 3]  346 		bne	timersdone	;greater than 0? do nothing
   0287 BD 06 1C      [ 8]  347 		jsr	SAUCER_INIT	;0 now? spawn a saucer
